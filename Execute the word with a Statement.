#include <stdio.h>
#include <string.h>

int main() {
    char input[100];
    
    // Read the full line including spaces
    fgets(input, sizeof(input), stdin);
    
    // Extract the first word
    char firstWord[100];
    sscanf(input, "%s", firstWord);

    // Print the desired output
    printf("May I know how to learn %s!!!...\n", firstWord);

    return 0;
}

