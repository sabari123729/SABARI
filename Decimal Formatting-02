#include <stdio.h>
#include <math.h>

int main() {
    float num;
    scanf("%f", &num);

    // Truncating to 2 decimal places
    float truncated = (int)(num * 100) / 100.0;

    // Rounding to 2 decimal places
    float rounded = roundf(num * 100) / 100;

    printf("%.2f\n", truncated);
    printf("%.2f\n", rounded);

    return 0;
}
