#include <stdio.h>
#include <string.h>

int main() {
    char str[1000];  // assuming input string won't exceed 999 characters
    scanf("%s", str);  // read string input

    int len = strlen(str);
    for (int i = len - 1; i >= 0; i--) {
        printf("%c", str[i]);
    }
    printf("\n");

    return 0;
}
